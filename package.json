{
    "name": "ftm-web",
    "private": true,
    "workspaces": [
        "apps/*"
    ],
    "scripts": {
        "check": "biome check --write .",
        "format": "biome format --write .",
        "dev": "turbo dev",
        "devs": "turbo dev -- --experimental-https",
        "build": "turbo build",
        "check-types": "turbo check-types",
        "dev:web": "turbo -F web dev",
        "postinstall": "node scripts/pin-deps.mjs --fix"
    },
    "dependencies": {
        "decimal.js": "10.6.0"
    },
    "devDependencies": {
        "@biomejs/biome": "2.1.2",
        "turbo": "2.5.4"
    },
    "packageManager": "pnpm@9.0.0"
}
